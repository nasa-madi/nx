{
    "name": "api",
    "description": "FeathersJS based API for NASA's MADI infrastructure",
    "version": "0.0.0",
    "homepage": "",
    "private": true,
    "keywords": [
        "feathers"
    ],
    "author": {},
    "contributors": [],
    "bugs": {},
    "engines": {
        "node": ">= 20.9.0"
    },
    "feathers": {
        "language": "js",
        "packager": "npm",
        "database": "postgresql",
        "framework": "koa",
        "transports": [
            "rest"
        ],
        "schema": "typebox"
    },
    "directories": {
        "lib": "src",
        "test": "test"
    },
    "main": "lib/index",
    "type": "module",
    "scripts": {
        "dev": "npm run dev:db && wait-on tcp:5432 && npm run dev:src",
        "dev:db": "docker-compose up -d database parser storage",
        "dev:src": "nodemon --exec 'npm start'",
        "dev:storage": "docker-compose up -d storage",
        "knex": "knex --debug",
        "migrate": "knex migrate:latest --debug",
        "migrate:make": "knex migrate:make",
        "mocha": "cross-env NODE_CONFIG_ENV=mocha mocha ./test/services/ --recursive --exit",
        "mocha:dev": "nodemon --exec npm run mocha",
        "prettier": "npx prettier \"**/*.js\" --write",
        "seed:admin": "knex seed:run --specific=dev-admin.js",
        "spec:build": "npm run spec:generate; npm run spec:merge; npm run spec:validate; npm run spec:swagger",
        "spec:generate": "npx foas generate",
        "spec:merge": "npx foas merge",
        "spec:mock": "npx @stoplight/prism-cli mock ./specifications/openapi/_merged.yml",
        "spec:swagger": "node ./node_modules/feathers-oas/node_modules/swagger-to-static/index.js ./specifications/openapi/_merged.yml ./public",
        "spec:validate": "npx @redocly/cli lint --extends=minimal ./specifications/openapi/_merged.yml",
        "start": "node --no-deprecation src",
        "debug": "node --inspect-brk=0.0.0.0:9229 --no-deprecation  src",
        "test": "cross-env NODE_ENV=mocha npm run migrate && npm run mocha",
        "test:contract": "npx newman run ./specifications/contract.postman_collection.json --folder setup --folder chunks",
        "test:dev": "node --test --watch ./test/**/*.test.js",
        "test:mocha": "cross-env NODE_ENV=test npm run mocha",
        "test:services:mocha:dev": "cross-env NODE_CONFIG_ENV=local-test npm run mocha",
        "test:services:node:dev": "cross-env NODE_ENV=mocha node --test --watch ./test/services/**/*.test.js"
    },
    "dependencies": {
        "@casl/ability": "^6.5.0",
        "@feathersjs/adapter-commons": "^5.0.24",
        "@feathersjs/authentication": "^5.0.24",
        "@feathersjs/authentication-client": "^5.0.24",
        "@feathersjs/authentication-local": "^5.0.24",
        "@feathersjs/authentication-oauth": "^5.0.24",
        "@feathersjs/commons": "^5.0.31",
        "@feathersjs/configuration": "^5.0.24",
        "@feathersjs/errors": "^5.0.24",
        "@feathersjs/feathers": "^5.0.24",
        "@feathersjs/knex": "^5.0.24",
        "@feathersjs/koa": "^5.0.24",
        "@feathersjs/schema": "^5.0.24",
        "@feathersjs/transport-commons": "^5.0.24",
        "@feathersjs/typebox": "^5.0.24",
        "@google-cloud/logging-winston": "^6.0.0",
        "@google-cloud/storage": "^7.7.0",
        "@koa/multer": "^3.0.2",
        "@sinclair/typebox": "^0.32.20",
        "ajv": "^8.14.0",
        "dauria": "^2.0.0",
        "dotenv": "^16.3.1",
        "feathers-blob": "^2.6.0",
        "feathers-casl": "^2.1.1",
        "feathers-hooks-common": "^8.1.2",
        "feathers-rpc": "^1.0.5",
        "flat": "^6.0.1",
        "form-data": "^4.0.0",
        "formdata-node": "^6.0.3",
        "formdata-polyfill": "^4.0.10",
        "fs-blob-store": "^6.0.0",
        "google-auth-library": "^8.9.0",
        "googleapis": "^144.0.0",
        "js-yaml": "^4.1.0",
        "knex": "^3.0.1",
        "knex-migrate": "^1.7.4",
        "koa-mount": "^4.0.0",
        "koa-qs": "^3.0.0",
        "langchain": "^0.1.25",
        "lodash": "^4.17.21",
        "madi-plg-base-class": "^0.0.1",
        "multer": "^1.4.5-lts.1",
        "node-fetch": "^3.3.2",
        "openai": "^4.20.0",
        "p-map": "^7.0.1",
        "pg": "^8.11.3",
        "pgvector": "^0.1.7",
        "qs": "^6.13.1",
        "sbd-splitter": "^0.1.1",
        "swagger-ui": "^5.17.14",
        "turndown": "^7.1.3",
        "wait-on": "^8.0.2",
        "winston": "^3.11.0"
    },
    "devDependencies": {
        "0x": "^5.7.0",
        "@endo/compartment-mapper": "^1.0.1",
        "@endo/static-module-record": "^1.0.1",
        "@feathersjs/cli": "^5.0.24",
        "@feathersjs/rest-client": "^5.0.24",
        "@stoplight/prism-cli": "^5.5.2",
        "@tootallnate/quickjs-emscripten": "^0.23.0",
        "assert": "^2.1.0",
        "axios": "^1.6.2",
        "chai": "^5.0.0",
        "concurrently": "^8.2.2",
        "cross-env": "^7.0.3",
        "esbuild": "^0.19.11",
        "feathers-oas": "^0.0.2",
        "mocha": "^10.2.0",
        "newman": "^6.1.2",
        "nock": "^13.5.4",
        "nodemon": "^3.0.1",
        "patch-package": "^8.0.0",
        "pg-mem": "^3.0.4",
        "prettier": "^3.1.0",
        "prism": "^1.0.0",
        "ses": "^1.0.1",
        "sinon": "^18.0.1",
        "sqlite3": "^5.1.7",
        "stacktracey": "^2.1.8"
    },
    "nodemonConfig": {
        "ignoreRoot": [
            ".git",
            "node_modules",
            ".cicd",
            "ignore"
        ]
    }
}
